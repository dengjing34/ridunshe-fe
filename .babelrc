{
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-runtime",
      "transform-vue-jsx",
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": ["env", "stage-2"],
        "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
      }
    }
  }